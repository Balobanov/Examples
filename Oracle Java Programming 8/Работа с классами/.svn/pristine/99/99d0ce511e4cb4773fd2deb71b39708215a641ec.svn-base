package ua.org.ao.balobanov;

class Student
{
	private String fName;
	private String sName;
	private int course;
	/**
	 * @param fName
	 * @param sNmae
	 * @param course
	 */
	public Student(String fName, String sName, int course) {
		super();
		this.fName = fName;
		this.sName = sName;
		this.course = course;
	}
	/**
	 * @return the fName
	 */
	public String getfName() {
		return fName;
	}
	/**
	 * @param fName the fName to set
	 */
	public void setfName(String fName) {
		this.fName = fName;
	}
	/**
	 * @return the sNmae
	 */
	public String getsName() {
		return sName;
	}
	/**
	 * @param sNmae the sNmae to set
	 */
	public void setsNmae(String sName) {
		this.sName = sName;
	}
	/**
	 * @return the course
	 */
	public int getCourse() {
		return course;
	}
	/**
	 * @param course the course to set
	 */
	public void setCourse(int course) {
		this.course = course;
	}
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "Student: " + getfName() + " " + getsName() + " course " + getCourse();
	}
	
}
